{
  "manifest_version": 2,
  "name": "React Chrome Extension Boilerplate",
  "description": "React boilerplate for Chrome extensions.",
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_title": "React Chrome Extension Boilerplate",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": ["storage"],
  "web_accessible_resources": ["*.svg", "*.jpg", "*.png", "*.gif"]
}
